{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Service Catalog Tag options (fdp-1qj64b360)",
	"Parameters": {
		"PortfolioId":{
            "Type":"String",
            "Description":"The SC portfolio to associate with this tag option library."
        }
	},
    "Resources": {
      "productEnvironmentProd" : {
        "Type" : "AWS::ServiceCatalog::TagOption",
        "Properties" : {
          "Active" : true,
          "Value" : "PRODUCTION",
          "Key" : "serviceEnvironment"
        }
      },
	  "productEnvironmentProdassoc":{
	    "Type" : "AWS::ServiceCatalog::TagOptionAssociation",
	    "Properties" : {
	  	  "ResourceId" : {"Ref": "PortfolioId"},
	  	  "TagOptionId" : { "Ref" : "productEnvironmentProd" }
	  	}
	  },
	  "productEnvironmentStaging" : {
        "Type" : "AWS::ServiceCatalog::TagOption",
        "Properties" : {
          "Active" : true,
          "Value" : "STAGING",
          "Key" : "serviceEnvironment"
        }
      },
	  "productEnvironmentStagingassoc":{
	    "Type" : "AWS::ServiceCatalog::TagOptionAssociation",
	    "Properties" : {
	  	  "ResourceId" : {"Ref": "PortfolioId"},
	  	  "TagOptionId" : { "Ref" : "productEnvironmentStaging" }
	  	}
	  },
      "productEnvironmentDevelopment" : {
        "Type" : "AWS::ServiceCatalog::TagOption",
        "Properties" : {
          "Active" : true,
          "Value" : "DEVELOPMENT",
          "Key" : "serviceEnvironment"
        }
      },
	  "productEnvironmentDevelopmentassoc":{
	    "Type" : "AWS::ServiceCatalog::TagOptionAssociation",
	    "Properties" : {
	  	  "ResourceId" : {"Ref": "PortfolioId"},
	  	  "TagOptionId" : { "Ref" : "productEnvironmentDevelopment" }
	  	}
	  },
      "productEnvironmentSandbox" : {
        "Type" : "AWS::ServiceCatalog::TagOption",
        "Properties" : {
          "Active" : true,
          "Value" : "SANDBOX",
          "Key" : "serviceEnvironment"
        }
      },
	  "productEnvironmentSandboxassoc":{
	    "Type" : "AWS::ServiceCatalog::TagOptionAssociation",
	    "Properties" : {
	  	  "ResourceId" : {"Ref": "PortfolioId"},
	  	  "TagOptionId" : { "Ref" : "productEnvironmentSandbox" }
	  	}
    },
      "ChargeBackDataInsights" : {
        "Type" : "AWS::ServiceCatalog::TagOption",
        "Properties" : {
          "Active" : true,
          "Value" : "DATA-INSIGHTS",
          "Key" : "Chargeback"
        }
      },
	  "ChargeBackDataInsightsassoc":{
	    "Type" : "AWS::ServiceCatalog::TagOptionAssociation",
	    "Properties" : {
	  	  "ResourceId" : {"Ref": "PortfolioId"},
	  	  "TagOptionId" : { "Ref" : "ChargeBackDataInsights" }
	  	}
	  },
      "ChargeBackAccounting" : {
        "Type" : "AWS::ServiceCatalog::TagOption",
        "Properties" : {
          "Active" : true,
          "Value" : "ACCOUNTING",
          "Key" : "Chargeback"
        }
      },
	  "ChargeBackAccountingassoc":{
	    "Type" : "AWS::ServiceCatalog::TagOptionAssociation",
	    "Properties" : {
	  	  "ResourceId" : {"Ref": "PortfolioId"},
	  	  "TagOptionId" : { "Ref" : "ChargeBackAccounting" }
	  	}
	  }
    }
}
